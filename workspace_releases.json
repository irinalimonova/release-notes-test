layout: null
---
{
  "show": {{ site.collections.workspace_releases.show | jsonify }},
  "items": [
    {% for item in site.workspace_releases | where_exp:"item","item.draft != true" %}
      {
        "version": {{ item.title | jsonify }},
        "description": {{ item.content | jsonify }}
      }{% if forloop.last == false %},{% endif %}
    {% endfor %}
  ]
}
